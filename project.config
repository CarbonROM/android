[access]
	inheritFrom = All-Projects
[access "refs/*"]
	owner = group user/Cisco R. (TheNameIsNigel)
[access "refs/for/refs/*"]
	read = group user/Cisco R. (TheNameIsNigel)
	push = group user/Cisco R. (TheNameIsNigel)
	label-Code-Review = -2..+2 group user/Cisco R. (TheNameIsNigel)
	label-Verified = -1..+1 group user/Cisco R. (TheNameIsNigel)
	rebase = group user/Cisco R. (TheNameIsNigel)
	removeReviewer = group user/Cisco R. (TheNameIsNigel)
	submit = group user/Cisco R. (TheNameIsNigel)
[access "refs/heads/*"]
	read = group user/Cisco R. (TheNameIsNigel)
	create = group user/Cisco R. (TheNameIsNigel)
	push = +force group user/Cisco R. (TheNameIsNigel)
	pushMerge = group user/Cisco R. (TheNameIsNigel)
[access "refs/tags/*"]
	push = +force group user/Cisco R. (TheNameIsNigel)
	createTag = group user/Cisco R. (TheNameIsNigel)
	createSignedTag = group user/Cisco R. (TheNameIsNigel)
