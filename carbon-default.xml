<?xml version="1.0" encoding="UTF-8"?>
<manifest>

  <remote  name="carbon"
           fetch="git://github.com/"
           review="review.carbonrom.org"
           revision="refs/heads/cr-10.0"
           sync-j="8" />

  <remote  name="gh"
           fetch="git://github.com/"
           sync-c="true"
           sync-j="8" />

  <remote  name="private"
           fetch="ssh://git@github.com" />

  <remote  name="lineage"
           fetch=".."
